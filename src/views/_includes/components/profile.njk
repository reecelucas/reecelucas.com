<section class="o-banner o-banner--contrast o-banner--contrast-asym">
  <div class="o-wrapper o-wrapper--no-pad">
    <div class="o-layout o-layout--no-gutter o-layout--col:2@med">
      <div class="o-wrapper">
        <img src="{{ profile.img.url }}" alt="{{ profile.img.alt }}">
      </div>
      <div class="o-wrapper">
        {% for paragraph in profile.paragraphs %}
          <p class="u-colour-beta">{{ paragraph }}</p>
        {% endfor %}
      </div>
    </div>
  </div>
</section>